name:          hasktorch-raw-types
version:       0.0.1.0
synopsis:      Raw C-types from torch.
description:   core C types
homepage:      https://github.com/hasktorch/hasktorch#readme
license:       BSD3
author:        Austin Huang
build-type:    Simple
cabal-version: >=1.10

library
  Build-tools: c2hs
  default-language: Haskell2010
  hs-source-dirs: src
  exposed-modules:
      THTypes
    , TorchStructs
  build-depends:
      base >= 4.7 && < 5
--    , inline-c >= 0.5

